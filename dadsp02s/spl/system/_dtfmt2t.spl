_dtfmt2t(fmt)
{
	local t;

	switch (fmt) {
		case 8:
			t = 1;
			break;

		case 9:
			t = 2;
			break;

		case 10:
			t = 3;
			break;

		case 13:
			t = 4;
			break;

		case 23:
			t = 5;
			break;

		case 24:
			t = 6;
			break;

		case 25:
			t = 7;
			break;

		default:
			t = 0;
			break;
	}
	return(t);
}


